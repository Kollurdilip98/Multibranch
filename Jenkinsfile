pipeline
{
    agent any
    
    stages()
    {
        stage('download')
        {
            steps()
            {
                git 'https://github.com/Kollurdilip98/Developer-Code.git'
            
            }
              }
        stage('build')
        {
            steps()
            {
                sh 'mvn package'
            }
        }
           
         
    }
}  
